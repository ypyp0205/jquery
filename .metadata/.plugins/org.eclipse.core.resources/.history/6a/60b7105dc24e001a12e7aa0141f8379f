		})
		
			
		$('#children').click(function(){
			$('#d1').contents('span').css('border', '1px solid blue');
		})
		
			
		$('#find').click(function(){
			$('#d1').contents('span').css('border', '1px solid green');
		})
		
		
		
	})
</script>
